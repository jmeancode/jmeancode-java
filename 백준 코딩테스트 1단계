package study;

import java.util.Scanner;

public class _01_1000 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, b;
		a = sc.nextInt();
		b = sc.nextInt();
		System.out.println(a-b);
		
	}

}

//두 정수 A와 B를 입력받은 다음, A+B를 출력하는 프로그램을 작성하시오.

package study;

import java.util.Scanner;

public class _01_1001 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, b;
		a = sc.nextInt();
		b = sc.nextInt();
		System.out.println(a+b);
		
	}

}
