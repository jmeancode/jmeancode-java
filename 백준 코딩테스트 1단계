//두 정수 A와 B를 입력받은 다음, A×B를 출력하는 프로그램을 작성하시오.
//오타


package study;

import java.util.Scanner;

public class _01_1008 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
        String id = scanner.nextLine();
        
        System.out.println(id+"??!");
       
	}

}
//준하는 사이트에 회원가입을 하다가 joonas라는 아이디가 이미 존재하는 것을 
//보고 놀랐다. 준하는 놀람을 ??!로 표현한다. 준하가 가입하려고 하는 
//사이트에 이미 존재하는 아이디가 주어졌을 때, 놀람을 표현하는 프로그램을 작성하시오.

//첫째 줄에 준하가 가입하려고 하는 사이트에 
//이미 존재하는 아이디가 주어진다. 
//아이디는 알파벳 소문자로만 이루어져 있으며,
//길이는 50자를 넘지 않는다.

//string 을 char로 하고, string을 입력받을 때 scanner.nextLine();
//인 걸 몰랐다.

package study;

import java.util.Scanner;

public class _01_10430 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, b, c, d, e, f, one, two, three, four;
		a = sc.nextInt();
		b = sc.nextInt();	
		c = sc.nextInt();	
		d = sc.nextInt();
		e = sc.nextInt();	
		f = sc.nextInt();	
	
		one = a*b*c*f;
		two = a*b*c*e*10;
		three = a*b*c*d*100;
		four = one + two + three;
		System.out.println(one);
		System.out.println(two);
		System.out.println(three);
		System.out.println(four);

		
	}

}
//두 정수 A와 B를 입력받은 다음, A×B를 출력하는 프로그램을 작성하시오.

package study;

import java.util.Scanner;

public class _01_10869 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, b;
		a = sc.nextInt();
		b = sc.nextInt();
		System.out.println((double)a/(double)b);
		
	}

}
//두 정수 A와 B를 입력받은 다음, A/B를 출력하는 프로그램을 작성하시오.
//첫째 줄에 A/B를 출력한다. 실제 정답과 출력값의 절대오차 또는 상대오차가 10-9 이하이면 정답이다.

//double로 형변환을 해야함. 그걸 몰랐음.


package study;

import java.util.Scanner;

public class _01_10988 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, b;
		a = sc.nextInt();
		b = sc.nextInt();
		System.out.println(a+b);
		
	}

}
//두 정수 A와 B를 입력받은 다음, A×B를 출력하는 프로그램을 작성하시오.

package study;

import java.util.Scanner;

public class _01_18108 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt(); 
		int a = 543;
		System.out.println(num - a);
		
	}

}

// 처음으로 한번에 맞음ㅎㅎ
//숫자면 int num = sc.nextInt();
//문자면 String st = sc.nextLine(); 기억하고 char이랑 헷갈리지 않기
